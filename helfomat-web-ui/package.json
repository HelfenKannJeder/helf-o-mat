  "angular-cli": {},
  "scripts": {
    "start": "./node_modules/.bin/ng serve --proxy-config proxy.conf.json",
    "start:prod": "./node_modules/.bin/ng serve --configuration production --proxy-config proxy.conf.json",
    "start:offline": "./node_modules/.bin/ng serve -e offline --proxy-config proxy.conf.json",
    "start:kiosk": "./node_modules/.bin/ng serve --configuration=kiosk --host 0.0.0.0 --port 4201 --proxy-config proxy.conf.json",
    "start:kiosk:prod": "./node_modules/.bin/ng serve --configuration=kiosk-production --host 0.0.0.0  --port 4201 --proxy-config proxy.conf.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "./node_modules/.bin/ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "postinstall": "./node_modules/.bin/ng build --configuration production --build-optimizer --output-path=target/client --base-href=/helf-o-mat/",
    "docker": "rm -rf docker/html && npm install && ./node_modules/.bin/ng build --configuration production --build-optimizer --output-path=docker/html/helf-o-mat --base-href=/helf-o-mat/",
    "build-prod": "./node_modules/.bin/ng build --configuration production --build-optimizer --base-href=/helf-o-mat/",
    "build-prod:kiosk": "./node_modules/.bin/ng build --configuration=kiosk-production --build-optimizer --base-href=/"
  },
  "private": true,
